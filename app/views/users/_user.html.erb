<tr >
		<% if user.id != current_user.id%>
			<td>
			<%= user.name %>
			</td>
			<td>
			<%= user.email %>
			</td>
			<td>
			<%= user.company_id %>
			</td>
			<td>
					<%= link_to 'Show', '#' %>
			</td>
			<td>
					<%= link_to 'Edit', edit_user_path(user) %>
			</td>
			<td>
					<%= link_to 'Delete',  user, :method => :delete, :data => { :confirm => 'Are you sure?' } %>
			</td>
		<% end %>
</tr>
